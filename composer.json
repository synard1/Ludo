{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "aws/aws-sdk-php": "^3.293",
        "barryvdh/laravel-snappy": "^1.0",
        "doctrine/dbal": "~3.5 || ~3.6.2",
        "geoip2/geoip2": "^2.13",
        "guzzlehttp/guzzle": "^7.2",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "infyomlabs/adminlte-templates": "^6.0",
        "infyomlabs/generator-builder": "^1.0",
        "infyomlabs/laravel-generator": "^6.0",
        "infyomlabs/laravel-ui-adminlte": "^5.2",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.0",
        "laravel/socialite": "^5.6",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.7",
        "laravelcollective/html": "^6.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.12",
        "maatwebsite/excel": "^3.1",
        "mediconesystems/livewire-datatables": "dev-master",
        "mhmiton/laravel-modules-livewire": "1.6.0",
        "nesbot/carbon": "^2.68",
        "nwidart/laravel-modules": "^10.0",
        "predis/predis": "^2.2",
        "psr/simple-cache": "^1.0",
        "ramsey/uuid": "^4.7",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-html": "^3.3",
        "spatie/laravel-permission": "^5.10",
        "yajra/laravel-datatables": "^10.0",
        "yajra/laravel-datatables-buttons": "^10.0",
        "yajra/laravel-datatables-html": "^10.7",
        "yajra/laravel-datatables-oracle": "^10.3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.9.1",
        "laravel/breeze": "^1.14",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Helpers/ModuleHelper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
