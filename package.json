{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "build": "webpack --max-workers 8",
    "build:js": "node build-scripts/build-js.js",
    "minify-js": "find public/assets/js/custom/ -name '*.js' -exec sh -c 'mkdir -p $(dirname {})/minified/ && terser {} --compress --mangle --output $(dirname {})/minified/$(basename {} .js).min.js' \\;",
    "obfuscate-js": "find public/assets/js/custom/apps/ -name '*.js' -exec sh -c 'mkdir -p $(dirname {})/obfuscated/ && terser {} --compress --mangle --output $(dirname {})/obfuscated/$(basename {} .js).min.js && javascript-obfuscator $(dirname {})/obfuscated/$(basename {} .js).min.js --output $(dirname {})/obfuscated/$(basename {} .js).obfuscated.js' \\;",
    "minify": "terser public/assets/js/custom/*.js --compress --mangle --output public/js/minified",
    "obfuscate": "javascript-obfuscator public/js/minified/*.js --output public/js/obfuscated",
    "build:minify": "npm run minify && npm run obfuscate"
  },
